<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#F5B800" />            
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#787" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="#AC3333" android:startColor="#983333" />
        </shape>
   	</item>
</selector>